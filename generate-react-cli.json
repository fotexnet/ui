{
  "usesTypeScript": true,
  "usesCssModule": true,
  "usesStyledComponents": false,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStory": true,
      "withLazy": false,
      "withStyle": false,
      "withTest": false,
      "customTemplates": {
        "component": "templates/component/TemplateName.component.tsx",
        "story": "templates/component/TemplateName.stories.tsx"
      }
    },
    "hook": {
      "path": "src/hooks",
      "withStory": false,
      "withLazy": false,
      "withStyle": false,
      "withTest": true,
      "customTemplates": {
        "component": "templates/hook/TemplateName.hook.tsx",
        "test": "templates/hook/TemplateName.test.tsx"
      }
    }
  }
}
